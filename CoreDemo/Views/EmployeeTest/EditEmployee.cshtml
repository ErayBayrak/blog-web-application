@model CoreDemo.Controllers.Class1
@{
    ViewData["Title"] = "EditEmployee";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>Çalışan Güncelleme</h1>
<form method="post">
    <label>ID</label>
    @Html.TextBoxFor(x => x.EmployeeID, new {@class="form-control"})
    <br />
    <label>Personel</label>
    @Html.TextBoxFor(x => x.EmployeeName, new {@class="form-control"})
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>
